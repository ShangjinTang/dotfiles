    local commands; commands=()
    _describe -t commands 'pueue edit commands' commands "$@"
}
(( $+functions[_pueue__help__edit_commands] )) ||
_pueue__help__edit_commands() {
    local commands; commands=()
    _describe -t commands 'pueue help edit commands' commands "$@"
}
