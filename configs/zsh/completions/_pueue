}
(( $+functions[_pueue__help__completions_commands] )) ||
_pueue__help__completions_commands() {
    local commands; commands=()
    _describe -t commands 'pueue help completions commands' commands "$@"
}
(( $+functions[_pueue__edit_commands] )) ||
